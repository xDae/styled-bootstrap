{
  "name": "styled-bootstrap",
  "version": "0.1.4",
  "private": false,
  "description": "--",
  "main": "lib/main.js",
  "module": "lib/main.es.js",
  "author": "Jose Miguel Bejarano <yo@josemiguel.org>",
  "homepage": "https://github.com/xDae/styled-bootstrap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xDae/styled-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/xDae/styled-bootstrap/issues"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "rimraf lib && rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier": "prettier --single-quote --write 'src/**/*.js'",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "rimraf docs && build-storybook -o docs",
    "lint:css": "stylelint './src/components/**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "styled-components": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.1.6",
    "@storybook/addon-centered": "^3.1.2",
    "@storybook/addon-links": "^3.1.6",
    "@storybook/addon-options": "^3.1.6",
    "@storybook/addons": "^3.1.6",
    "@storybook/react": "^3.1.6",
    "aria-query": "^0.7.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.48.0",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "jest-styled-components": "^3.1.5",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dev-utils": "^3.0.0",
    "react-dom": "^15.6.1",
    "react-transition-group": "^1.2.0",
    "rimraf": "2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.2"
  },
  "peerDependencies": {
    "react": "15.x",
    "prop-types": "15.x",
    "recompose": "^0.23.5",
    "polished": "^1.1.3"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(lib|docs|node_modules)/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ]
  }
}
